{
  "name": "nextee",
  "version": "0.0.10",
  "private": true,
  "scripts": {
    "dev": "next dev --port 7766",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cs": "changeset",
    "vs": "changeset version",
    "tag": "changeset tag",
    "rel": "standard-version --tag-prefix \"$(node -p \"require('./package.json').name\")@\" && git push --follow-tags",
    "release": "standard-version --first-release --tag-prefix \"$(node -p \"require('./package.json').name\")@\" && git push --follow-tags",
    "mg": "npx prisma migrate dev && npx prisma generate",
    "mg:prod": "npx prisma db push && npx prisma generate",
    "postinstalld": "pnpm mg:prod"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@as-integrations/next": "^3.0.0",
    "@auth/prisma-adapter": "^2.0.0",
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.1.3",
    "@prisma/client": "^5.13.0",
    "@tanstack/react-query": "^5.32.1",
    "@trpc/client": "11.0.0-rc.359",
    "@trpc/next": "11.0.0-rc.359",
    "@trpc/react-query": "11.0.0-rc.359",
    "@trpc/server": "11.0.0-rc.359",
    "apollo-server-micro": "^3.13.0",
    "graphql-tag": "^2.12.6",
    "next": "14.2.3",
    "next-auth": "5.0.0-beta.17",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.3",
    "react-hook-form-persist": "^3.0.0",
    "superjson": "^2.2.1",
    "zod": "^3.23.5"
