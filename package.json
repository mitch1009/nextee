    "@types/react-dom": "^18",
    "axios": "^1.6.8",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "prisma": "^5.13.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
